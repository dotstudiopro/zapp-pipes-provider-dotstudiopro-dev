{
  "name": "zapp-pipes-provider-dotstudiopro-dev",
  "version": "0.0.1",
  "description": "DEV Version - Zapp Provider for Applicaster platform by dotstudioPRO",
  "main": "lib/index.js",
  "scripts": {
    "test": "node_modules/.bin/jest",
    "test:integration": "node_modules/.bin/jest integration.test.js",
    "start": "node_modules/.bin/nodemon server.js --exec node_modules/.bin/babel-node",
    "start:hot": "node_modules/.bin/babel-node webpack-hot.js",
    "build": "rm -rf lib && NODE_ENV=production node_modules/.bin/babel src -d lib",
    "prepublish": "npm run build"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "axios": "0.21.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "@applicaster/zapp-pipes-dev-kit": "^1.3.0",
    "babel-cli": "^6.24.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.26.0",
    "jest": "^22.4.2",
    "nock": "^9.2.3",
    "nodemon": "^1.11.0",
    "ramda": "^0.25.0",
    "sinon": "^4.4.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.11.5"
  },
  "jest": {
    "testRegex": "/__tests__/.*(\\.test.js|\\test.jsx)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/node_modules"
    ],
    "collectCoverageFrom": [
      "packages/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotstudiopro/zapp-pipes-provider-dotstudiopro-dev.git"
  },
  "author": "dotstudioPRO",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/dotstudiopro/zapp-pipes-provider-dotstudiopro-dev/issues"
  },
  "homepage": "https://github.com/dotstudiopro/zapp-pipes-provider-dotstudiopro-dev#readme"
}
